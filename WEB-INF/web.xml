<?xml version="1.0" encoding="utf-8"?>
<web-app id="iocaste_session" xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5"> 
    <description>iocaste session</description>
    <display-name>iocaste session</display-name>
    
    <servlet>
        <servlet-name>iocaste_session</servlet-name>
        <servlet-class>org.iocaste.session.SessionServlet</servlet-class>
    </servlet>
    
    
    <servlet-mapping>
        <servlet-name>iocaste_session</servlet-name>
        <url-pattern>/session.jsp</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>iocaste_calculadora</servlet-name>
        <servlet-class>org.iocaste.session.CalculadoraServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>iocaste_calculadora</servlet-name>
        <url-pattern>/calculadora.jsp</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>iocaste_cadastro</servlet-name>
        <servlet-class>org.iocaste.session.CadastroServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>iocaste_cadastro</servlet-name>
        <url-pattern>/cadastro.jsp</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>iocaste_calculo</servlet-name>
        <servlet-class>org.iocaste.session.CalculoServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>iocaste_calculo</servlet-name>
        <url-pattern>/calculo.jsp</url-pattern>
    </servlet-mapping>
    
</web-app>